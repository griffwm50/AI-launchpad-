    <script>
        function showSection(sectionId, elementId = null) {
            document.querySelectorAll('.section').forEach(sec => {
                sec.classList.remove('active');
            });
            const targetSection = document.getElementById(sectionId);
            if (targetSection) {
                targetSection.classList.add('active');
            }
           
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            const activeNavItem = document.querySelector(`.nav-item[data-target="${sectionId}"]`);
            if (activeNavItem) {
                activeNavItem.classList.add('active');
            }

            const whichToolCollapsible = document.getElementById('which-tool-collapsible');

            if (sectionId === 'cheatsheet') {
                if (elementId) {
                    // Collapse the "Which Tool" section when linking to a specific tool
                    if (whichToolCollapsible && whichToolCollapsible.classList.contains('open')) {
                        whichToolCollapsible.classList.remove('open');
                        const icon = whichToolCollapsible.querySelector('.collapsible-header .material-icons:not(.material-icons-outlined)');
                        if (icon) icon.textContent = 'expand_more';
                    }

                    setTimeout(() => {
                        const element = document.getElementById(elementId);
                        if (element) {
                            const collapsible = element.closest('.collapsible-section');
                            if (collapsible && !collapsible.classList.contains('open')) {
                                collapsible.classList.add('open');
                                const icon = collapsible.querySelector('.collapsible-header .material-icons:not(.material-icons-outlined)');
                                if (icon) icon.textContent = 'expand_less';
                            }
                            element.scrollIntoView({ behavior: 'smooth', block: 'center' });
                            element.classList.add('highlight');
                            setTimeout(() => element.classList.remove('highlight'), 2500);
                        }
                    }, 100);
                } else {
                    // Expand the "Which Tool" section when navigating to the cheatsheet section
                    if (whichToolCollapsible && !whichToolCollapsible.classList.contains('open')) {
                        whichToolCollapsible.classList.add('open');
                        const icon = whichToolCollapsible.querySelector('.collapsible-header .material-icons:not(.material-icons-outlined)');
                        if (icon) icon.textContent = 'expand_less';
                    }
                    document.querySelector('.main-content').scrollTo(0, 0);
                }
            } else {
                document.querySelector('.main-content').scrollTo(0, 0);
            }
        }

        function createResultElement(itemElement, type) {
            const result = document.createElement('div');
            result.className = 'mb-2 p-2 border-b border-google-grey-200';
            const link = document.createElement('a');
            link.className = 'text-blue-600 hover:underline cursor-pointer';

            if (type === 'use-case') {
                const btn = itemElement;
                const useCaseName = btn.querySelector('h4').textContent;
                link.innerHTML = `<i class="material-icons-outlined nav-icon" style="font-size: 18px; vertical-align: middle;">assignment</i> See "<b>${useCaseName}</b>" in Use Cases`;
                link.onclick = (e) => {
                    e.preventDefault();
                    showSection('use-cases');
                    // Click the button to show details
                    setTimeout(() => {
                        btn.click();
                        btn.scrollIntoView({ behavior: 'smooth', block: 'center' });
                    }, 200); // Timeout to ensure the section is visible
                };
            } else { // tool
                const card = itemElement;
                const toolId = card.id;
                const toolName = card.querySelector('.tool-name').textContent;
                let description = "";
                // Add descriptions for tools
                switch (toolId) {
                    case 'gemini-workspace': description = " - to write, summarize, and analyze in Workspace apps"; break;
                    case 'ai-function': description = " - to analyze data directly within Sheets"; break;
                    case 'workspace-flows': description = " - to automate tasks across Workspace apps"; break;
                    case 'ai-snippets-machine': description = " - to generate draft weekly snippets"; break;
                    case 'llm-extension': description = " - to summarize or generate text on web pages"; break;
                    case 'plx-explorer': description = " - to interactively explore and visualize data"; break;
                    case 'notebooklm': description = " - to understand and summarize long documents"; break;
                    case 'duckie': description = " - to get answers about Google-specific things"; break;
                    case 'ai-studio': description = " - to experiment with powerful AI models and APIs"; break;
                    case 'gemini-gems': description = " - to create reusable custom AI assistants"; break;
                    case 'gdm-labs': description = " - to try out latest DeepMind model innovations"; break;
                    case 'gemma': description = " - to build on open, lightweight AI models"; break;
                    case 'vertex-ai': description = " - to build, train, and deploy ML models on Cloud"; break;
                    case 'agentspace': description = " - to develop and manage AI agents"; break;
                    case 'cider-agent': description = " - to get AI coding assistance in Cider V"; break;
                    case 'imagen': description = " - to create images from text"; break;
                    case 'veo': description = " - to generate video clips from text or images"; break;
                    case 'google-vids': description = " - to create videos within Workspace"; break;
                    default: description = "";
                }

                link.innerHTML = `<i class="material-icons-outlined nav-icon" style="font-size: 18px; vertical-align: middle;">list_alt</i> See Tool: "<b>${toolName}</b>"${description}`;
                link.onclick = (e) => {
                    e.preventDefault();
                    const toolCard = document.getElementById(toolId);
                    if (toolCard) {
                        const collapsible = toolCard.closest('.collapsible-section');
                        if (collapsible && !collapsible.classList.contains('open')) {
                            collapsible.classList.add('open');
                            const icon = collapsible.querySelector('.collapsible-header .material-icons:not(.material-icons-outlined)');
                            if (icon) icon.textContent = 'expand_less';
                        }
                    }
                    // Call showSection AFTER attempting to open the collapsible
                    showSection('cheatsheet', toolId);
                };
            }
            result.appendChild(link);
            return result;
        }

        function performSearch(term) {
            const searchTerm = term.toLowerCase().trim();
            const resultsContainer = document.getElementById('search-results');
            resultsContainer.innerHTML = '';

            if (searchTerm.length === 0) return;

            const fragment = document.createDocumentFragment();
            let hasResults = false;

            // Search Use Cases
            document.querySelectorAll('.use-case-btn').forEach(btn => {
                const title = btn.querySelector('h4').textContent.toLowerCase();
                const keywords = btn.dataset.keywords ? btn.dataset.keywords.toLowerCase() : '';
                if (title.includes(searchTerm) || keywords.includes(searchTerm)) {
                    const result = createResultElement(btn, 'use-case');
                    fragment.appendChild(result);
                    hasResults = true;
                }
            });

            // Search Tools
            document.querySelectorAll('#tool-list .tool-card').forEach(card => {
                const toolNameElement = card.querySelector('.tool-name');
                if (toolNameElement) {
                    const toolName = toolNameElement.textContent.toLowerCase();
                    const keywords = card.dataset.keywords ? card.dataset.keywords.toLowerCase() : '';
                    if (toolName.includes(searchTerm) || keywords.includes(searchTerm)) {
                        const result = createResultElement(card, 'tool');
                        fragment.appendChild(result);
                        hasResults = true;
                    }
                }
            });

            if (hasResults) {
                resultsContainer.appendChild(fragment);
            } else {
                resultsContainer.innerHTML = '<p class="text-google-grey-600">No results found.</p>';
            }
        }

        function addCollapsibleListeners(container) {
            container.querySelectorAll('.collapsible-header').forEach(header => {
                // Remove any existing listeners to avoid duplicates
                const newHeader = header.cloneNode(true);
                header.parentNode.replaceChild(newHeader, header);

                newHeader.addEventListener('click', (event) => {
                    event.stopPropagation();
                    const section = newHeader.closest('.collapsible-section');
                    if (section) {
                        section.classList.toggle('open');
                         const icon = newHeader.querySelector('.material-icons:not(.material-icons-outlined)');
                        if (icon) {
                            icon.textContent = section.classList.contains('open') ? 'expand_less' : 'expand_more';
                        }
                    }
                });
            });
        }

        document.addEventListener('DOMContentLoaded', () => {
            // Initialize first section
             const hash = window.location.hash.substring(1);
            const validSections = Array.from(document.querySelectorAll('.section')).map(s => s.id);
            if (hash && validSections.includes(hash)) {
                showSection(hash);
            } else {
                showSection('intro');
            }

            const navLinks = document.querySelectorAll('.nav-item');
            navLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();
                    const targetId = link.getAttribute('data-target');
                    if (targetId) {
                        showSection(targetId);
                        if (history.pushState) {
                             history.pushState(null, null, `#${targetId}`);
                        } else {
                             location.hash = `#${targetId}`;
                        }
                    }
                });
            });

            window.addEventListener('popstate', () => {
                const hash = window.location.hash.substring(1);
                 const validSections = Array.from(document.querySelectorAll('.section')).map(s => s.id);
                if (hash && validSections.includes(hash)) {
                    showSection(hash);
                } else {
                    showSection('intro');
                }
            });

            addCollapsibleListeners(document);

            const toolSearch = document.getElementById('tool-search');
            if (toolSearch) {
                toolSearch.addEventListener('input', () => {
                    const searchTerm = toolSearch.value.toLowerCase();
                    document.querySelectorAll('#tool-list .tool-card').forEach(card => {
                        const cardText = card.textContent.toLowerCase();
                        const keywords = card.dataset.keywords || '';
                        if (cardText.includes(searchTerm) || keywords.toLowerCase().includes(searchTerm)) {
                            card.style.display = 'block';
                        } else {
                            card.style.display = 'none';
                        }
                    });
                    document.querySelectorAll('#tool-list .collapsible-section').forEach(section => {
                        const content = section.querySelector('.collapsible-content');
                        if (!content) return;
                        const sectionCards = content.querySelectorAll('.tool-card');
                        let hasMatch = false;
                        sectionCards.forEach(card => {
                            if (card.style.display === 'block') {
                                hasMatch = true;
                            }
                        });
                        if (hasMatch && searchTerm.length > 0) {
                            if (!section.classList.contains('open')) {
                                section.classList.add('open');
                            }
                        }
                    });
                });
            }

            // Main search in Intro
            const mainSearchInput = document.getElementById('main-search');
            if (mainSearchInput) {
                mainSearchInput.addEventListener('input', () => {
                    performSearch(mainSearchInput.value);
                });
            }

            const useCaseBtns = document.querySelectorAll('.use-case-btn');
            const useCaseDetailsContainer = document.getElementById('use-case-details');
            const useCaseContent = {
